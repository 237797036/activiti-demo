          <activiti:value id="true" name="同意"></activiti:value>
          <activiti:value id="false" name="拒绝"></activiti:value>
        </activiti:formProperty>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow2" sourceRef="startevent1" targetRef="deptLeaderAudit"></sequenceFlow>
    <sequenceFlow id="flow7" sourceRef="deptLeaderAudit" targetRef="hrAudit"></sequenceFlow>
    <userTask id="managerAudit" name="总经理审批"></userTask>
    <sequenceFlow id="flow8" sourceRef="hrAudit" targetRef="managerAudit"></sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow9" sourceRef="managerAudit" targetRef="endevent2"></sequenceFlow>
